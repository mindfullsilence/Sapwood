{
  "name": "mindfullsilence/sapwood",
  "description": "A starter theme for using ACF, Timber, and a component-based development workflow.",
  "version": "0.0.2",
  "type": "wordpress-theme",
  "license": "MIT",
  "require": {
    "php": ">=7.0.0",
    "composer/installers": "~1.0",
    "timber/timber": "^1.2"
  },
  "autoload": {
    "psr-4": {
      "Sapwood\\": ["app/", "app/lib"],
      "Sapwood\\Library\\": "app/lib/core",
      "Sapwood\\Template\\": "app/public/templates",
      "Sapwood\\Component\\": "app/public/components"
    }
  }
}
